{
  "name": "frontend",
  "version": "0.0.3",
  "description": "PileaX AI",
  "productName": "PileaX",
  "code": "pileax",
  "author": "pileax <ipileax@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"**/*.{ts,js,cjs,mjs,vue}\"",
    "lint-fix": "yarn lint --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "dev:tauri": "cd src-tauri && cargo run",
    "build": "quasar build",
    "build:electron": "node_modules/.bin/electron-rebuild && quasar build -m electron",
    "build:electron:linux": "node_modules/.bin/electron-rebuild && quasar build -m electron --target=linux",
    "build:electron:win": "node_modules/.bin/electron-rebuild && quasar build -m electron --target=win",
    "build:electron:analyze": "export analyze=true && node_modules/.bin/electron-rebuild && quasar build -m electron",
    "build:tauri": "cd src-tauri && yarn tauri build",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
