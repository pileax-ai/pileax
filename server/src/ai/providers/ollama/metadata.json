{
  "name": "ollama",
  "title": "Ollama",
  "description": "Run LLMs locally on your own machine",
  "home": "https://ollama.com",
  "github": "https://github.com/ollama/ollama",
  "huggingface": "https://huggingface.co/ollama",
  "config": [
    {
      "name": "apiKey",
      "type": "password",
      "key": "OLLAMA_API_KEY",
      "value": "",
      "values": []
    },
    {
      "name": "model",
      "type": "select",
      "key": "OLLAMA_MODEL",
      "value": "",
      "values": [
        "deepseek-chat",
        "deepseek-reasoner"
      ]
    },
    {
      "name": "baseURL",
      "type": "test",
      "key": "OLLAMA_BASE_URL",
      "value": "",
      "values": []
    }
  ]
}
