{
	"name": "pileax-server",
	"version": "1.0.0",
	"description": "An Express boilerplate backend",
	"author": "Edwin Hernandez",
	"repository": "edwinhern/express-typescript-2024",
	"license": "MIT",
	"main": "index.ts",
	"private": true,
	"scripts": {
		"build": "tsup",
		"start:dev": "tsx watch --clear-screen=false src/index.ts | pino-pretty",
		"start:embed": "tsx watch --clear-screen=false src/embed.ts",
		"start:prod": "dotenv -e .env.production -- node dist/index.js",
		"lint": "biome lint",
		"lint:fix": "biome lint --write",
		"format": "biome format --write",
		"test": "vitest run",
		"test:cov": "vitest run --coverage",
		"check": "pnpm lint && pnpm format && pnpm build && pnpm test",
		"drizzle:generate": "drizzle-kit generate"
	},
	"dependencies": {
		"@asteasolutions/zod-to-openapi": "7.3.0",
		"@libsql/client": "^0.15.1",
		"better-sqlite3": "^11.9.1",
		"cors": "2.8.5",
		"dotenv": "16.4.7",
		"drizzle-orm": "^0.41.0",
		"envalid": "8.0.0",
		"express": "4.21.2",
		"express-async-errors": "^3.1.1",
		"express-jwt": "^8.5.1",
		"express-rate-limit": "7.5.0",
		"get-port": "^7.1.0",
		"helmet": "8.1.0",
		"http-status-codes": "2.3.0",
		"jsonwebtoken": "^9.0.2",
		"multer": "^1.4.5-lts.2",
		"openai": "^4.90.0",
		"pino": "9.6.0",
		"pino-http": "10.4.0",
		"ramda": "^0.30.1",
		"swagger-ui-express": "5.0.1",
		"uuid": "^11.1.0",
		"zod": "3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/better-sqlite3": "^7.6.12",
		"@types/cors": "2.8.17",
		"@types/express": "5.0.1",
		"@types/multer": "^1.4.12",
		"@types/ramda": "^0.30.2",
		"@types/supertest": "6.0.2",
		"@types/swagger-ui-express": "4.1.8",
		"@vitest/coverage-v8": "3.0.9",
		"drizzle-kit": "^0.30.6",
		"pino-pretty": "13.0.0",
		"supertest": "7.1.0",
		"tsup": "8.4.0",
		"tsx": "4.19.3",
		"typescript": "5.8.2",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.0.9"
	}
}
